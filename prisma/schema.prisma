generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"

}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")

}

model User {
  id Int @id @default(autoincrement())
  name String?
  login String @unique @db.VarChar(255)
  hash String @db.VarChar(255)
}


